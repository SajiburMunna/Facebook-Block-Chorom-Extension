{
    "version": "1.0",
    "manifest_version": 2,
    "name": "FbBlock",
    "description": "Helps you Block Facebook ",
    "permissions": [
        "webRequestBlocking",
        "webRequest",
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}